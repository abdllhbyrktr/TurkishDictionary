{
  "name": "Wordreference.com",
  "baseUrl": "http://www.wordreference.com/",
  "tabIndex": 1,
  "baseSelectors": [".WRD", "#article"],
  "conditionSelector": "",
  "multipleResults": true,
  "removeElements": [
    ".wrtopsection",
    ".POS2 span",
    ".small1"
  ],
  "attributes": [],
  "classes": [{
    "remove": [],
    "replace": [{
        "old": "even",
        "new": "wrEven"
      },
      {
        "old": "odd",
        "new": "wrOdd"
      }
    ]
  }],
  "slices": {
    "maxResults": 5,
    "unique": [
      ".WRD tr"
    ],
    "each": []
  },
  "organizeLinks": [
    "a"
  ],
  "audioUrlStatement": "var $listenWidget = $data.find('#listen_widget');$listenWidget.length ? 'http://wordrereference.com/' + $listenWidget.find('audio:first source').attr('src') : '';",
  "langUrls": {
    "en": {
      "tr": "entr/",
      "de": "ende/",
      "es": "es/translation.asp?tranword=",
      "fr": "enfr/",
      "ru": "enru/",
      "cn": "enzh/"
    },
    "tr": {
      "en": "tren/"
    },
    "de": {
      "en": "deen/"
    },
    "es": {
      "en": "es/en/translation.asp?spen=",
      "fr": "esfr/"
    },
    "fr": {
      "en": "fren/",
      "es": "fres/"
    },
    "ru": {
      "en": "ruen/"
    },
    "cn": {
      "en": "zhen/"
    }
  }
}